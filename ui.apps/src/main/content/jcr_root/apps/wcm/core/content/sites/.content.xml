<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            jcr:mixinTypes="[sling:VanityPath]"
            jcr:primaryType="nt:unstructured"
            jcr:title="AEM Sites"
            sling:resourceType="granite/ui/components/shell/collectionpage"
            sling:vanityOrder="{Long}301"
            sling:vanityPath="/sites"
            consoleId="cq-sites"
            modeGroup="cq-siteadmin-admin-childpages"
            omnisearchLocationPath="/libs/granite/omnisearch/content/metadata/site"
            pageURITemplate="/sites.html{+id}"
            targetCollection=".cq-siteadmin-admin-childpages">
        <actions jcr:primaryType="nt:unstructured">
            <secondary jcr:primaryType="nt:unstructured">
                <create
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/pulldown"
                        text="Create"
                        variant="primary">
                    <items jcr:primaryType="nt:unstructured">
                        <createpage
                                granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                                granite:rel="cq-siteadmin-admin-createpage"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                href="/mnt/overlay/wcm/core/content/sites/createpagewizard.html${requestPathInfo.suffix}"
                                icon="experience"
                                rel="cq-siteadmin-admin-createpage"
                                relScope="collection"
                                target=".cq-siteadmin-admin-childpages"
                                text="Page">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/wcm/core/content/sites/createpagewizard.html{+id}"/>
                        </createpage>
                        <createsite
                                granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                                granite:hidden="{Boolean}true"
                                granite:rel="cq-siteadmin-admin-createsite"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                href="/mnt/overlay/wcm/core/content/sites/createsitewizard.html${requestPathInfo.suffix}"
                                icon="selectContainer"
                                rel="cq-siteadmin-admin-createsite"
                                relScope="collection"
                                target=".cq-siteadmin-admin-childpages"
                                text="Site">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/wcm/core/content/sites/createsitewizard.html{+id}"/>
                        </createsite>
                        <createlivecopy
                                granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                                granite:hidden="{Boolean}true"
                                granite:rel="cq-siteadmin-admin-createlivecopy"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                href="/mnt/overlay/wcm/core/content/sites/createlivecopywizard.html${requestPathInfo.suffix}"
                                icon="pages"
                                rel="cq-siteadmin-admin-createlivecopy"
                                relScope="collection"
                                target=".cq-siteadmin-admin-childpages"
                                text="Live Copy">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/wcm/core/content/sites/createlivecopywizard.html{+id}"/>
                        </createlivecopy>
                        <createlaunch
                                granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                                granite:hidden="{Boolean}true"
                                granite:rel="cq-siteadmin-admin-createlaunch"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                href="/mnt/overlay/wcm/core/content/sites/createlaunchwizard.html${requestPathInfo.suffix}"
                                icon="launch"
                                rel="cq-siteadmin-admin-createlaunch"
                                relScope="collection"
                                target=".cq-siteadmin-admin-childpages"
                                text="Launch">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/wcm/core/content/sites/createlaunchwizard.html{+id}"/>
                        </createlaunch>
                        <createlanguagecopy
                                granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                                granite:hidden="{Boolean}true"
                                granite:rel="cq-siteadmin-admin-createlanguagecopy"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                href="/mnt/overlay/wcm/core/content/sites/createlanguagecopywizard.html${requestPathInfo.suffix}"
                                icon="project"
                                rel="cq-siteadmin-admin-createlanguagecopy"
                                relScope="collection"
                                target=".cq-siteadmin-admin-childpages"
                                text="Language Copy">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/wcm/core/content/sites/createlanguagecopywizard.html"/>
                            <granite:rendercondition
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="cq/translation/languagecopyrendercondition"/>
                        </createlanguagecopy>
                        <createcatalog
                                granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                                granite:hidden="{Boolean}true"
                                granite:rel="cq-siteadmin-admin-createcatalog"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                href="/mnt/overlay/commerce/gui/content/catalogs/createcatalogwizard.html${requestPathInfo.suffix}"
                                icon="fileTemplate"
                                rel="cq-siteadmin-admin-createcatalog"
                                relScope="collection"
                                target=".cq-siteadmin-admin-childpages"
                                text="Catalog">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/commerce/gui/content/catalogs/createcatalogwizard.html{+id}"/>
                        </createcatalog>
                        <createfolder
                                granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                                granite:rel="cq-siteadmin-admin-createfolder"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.dialog"
                                href="/mnt/overlay/wcm/core/content/coral/common/createfolderdialog.html${requestPathInfo.suffix}"
                                icon="folder"
                                rel="cq-siteadmin-admin-createfolder"
                                relScope="collection"
                                target=".cq-siteadmin-admin-childpages"
                                text="Folder">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    nesting="hide"
                                    src.uritemplate="/mnt/overlay/wcm/core/content/coral/common/createfolderdialog.html{+id}"/>
                        </createfolder>
                        <createcsvexport
                                granite:hidden="{Boolean}true"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                icon="report"
                                rel="cq-siteadmin-admin-createcsvexport"
                                relScope="none"
                                target=".cq-siteadmin-admin-childpages"
                                text="CSV Report">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/wcm/core/content/sites/createcsvexport.html{+id}"/>
                        </createcsvexport>
                    </items>
                </create>
            </secondary>
            <selection jcr:primaryType="nt:unstructured">
                <create
                        granite:class="foundation-collection-action"
                        granite:rel="cq-siteadmin-admin-actions-create-activator"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/pulldown"
                        icon="addCircle"
                        text="Create"
                        variant="actionBar">
                    <granite:data
                            jcr:primaryType="nt:unstructured"
                            foundation-collection-action="\{&quot;target&quot;: &quot;.cq-siteadmin-admin-childpages&quot;, &quot;activeSelectionCount&quot;: &quot;multiple&quot;}"/>
                    <items jcr:primaryType="nt:unstructured">
                        <createworkflow
                                granite:rel="cq-siteadmin-admin-createworkflow"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                activeSelectionCount="multiple"
                                icon="layersBringToFront"
                                target=".cq-siteadmin-admin-childpages"
                                text="Workflow">
                            <granite:rendercondition
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="cq/gui/components/renderconditions/canreadworkflowmodels"/>
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/override/libs/wcm/core/content/common/startbulkworkflows.html{?item*}"/>
                        </createworkflow>
                        <createversion
                                granite:rel="cq-siteadmin-admin-createversion"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.dialog"
                                activeSelectionCount="multiple"
                                icon="layers"
                                target=".cq-siteadmin-admin-childpages"
                                text="Version">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    nesting="hide"
                                    src.uritemplate="/mnt/overlay/wcm/core/content/coral/common/createversiondialog.html"/>
                        </createversion>
                        <createpage
                                granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                                granite:rel="cq-siteadmin-admin-createpage"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                activeSelectionCount="single"
                                href="/mnt/overlay/wcm/core/content/sites/createpagewizard.html${requestPathInfo.suffix}"
                                icon="experience"
                                rel="cq-siteadmin-admin-createpage"
                                relScope="collection"
                                target=".cq-siteadmin-admin-childpages"
                                text="Page">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/wcm/core/content/sites/createpagewizard.html{+item}"/>
                        </createpage>
                        <createsite
                                granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                                granite:hidden="{Boolean}true"
                                granite:rel="cq-siteadmin-admin-createsite"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                activeSelectionCount="single"
                                href="/mnt/overlay/wcm/core/content/sites/createsitewizard.html${requestPathInfo.suffix}"
                                icon="selectContainer"
                                rel="cq-siteadmin-admin-createsite"
                                relScope="collection"
                                target=".cq-siteadmin-admin-childpages"
                                text="Site">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/wcm/core/content/sites/createsitewizard.html{+id}"/>
                        </createsite>
                        <createlivecopy
                                granite:hidden="{Boolean}true"
                                granite:rel="cq-siteadmin-admin-createlivecopy"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                activeSelectionCount="single"
                                icon="pages"
                                target=".cq-siteadmin-admin-childpages"
                                text="Live Copy">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/wcm/core/content/sites/createlivecopywizard.html{+id}?source={item}"/>
                        </createlivecopy>
                        <createlaunch
                                granite:hidden="{Boolean}true"
                                granite:rel="cq-siteadmin-admin-createlivecopy"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                activeSelectionCount="multiple"
                                icon="launch"
                                target=".cq-siteadmin-admin-childpages"
                                text="Launch">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/wcm/core/content/sites/createlaunchwizard.html{+id}?source={item}"/>
                        </createlaunch>
                        <createlanguagecopy
                                granite:hidden="{Boolean}true"
                                granite:rel="cq-siteadmin-admin-createlanguagecopy"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                activeSelectionCount="multiple"
                                icon="project"
                                target=".cq-siteadmin-admin-childpages"
                                text="Language Copy">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/wcm/core/content/sites/createlanguagecopywizard.html?source={item}"/>
                            <granite:rendercondition
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="cq/translation/languagecopyrendercondition"/>
                        </createlanguagecopy>
                        <createcatalog
                                granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                                granite:hidden="{Boolean}true"
                                granite:rel="cq-siteadmin-admin-createcatalog"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                href="/mnt/overlay/commerce/gui/content/catalogs/createcatalogwizard.html${requestPathInfo.suffix}"
                                icon="fileTemplate"
                                rel="cq-siteadmin-admin-createcatalog"
                                relScope="collection"
                                target=".cq-siteadmin-admin-childpages"
                                text="Catalog">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/commerce/gui/content/catalogs/createcatalogwizard.html{+id}"/>
                        </createcatalog>
                        <createfolder
                                granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                                granite:rel="cq-siteadmin-admin-createfolder"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.dialog"
                                activeSelectionCount="single"
                                href="/mnt/overlay/wcm/core/content/coral/common/createfolderdialog.html${requestPathInfo.suffix}"
                                icon="folder"
                                rel="cq-siteadmin-admin-createfolder"
                                relScope="collection"
                                target=".cq-siteadmin-admin-childpages"
                                text="Folder">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    nesting="hide"
                                    src.uritemplate="/mnt/overlay/wcm/core/content/coral/common/createfolderdialog.html{+id}"/>
                        </createfolder>
                        <createcsvexport
                                granite:hidden="{Boolean}true"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                                action="foundation.link"
                                activeSelectionCount="single"
                                icon="report"
                                rel="cq-siteadmin-admin-createcsvexport"
                                relScope="none"
                                target=".cq-siteadmin-admin-childpages"
                                text="CSV Report">
                            <data
                                    jcr:primaryType="nt:unstructured"
                                    href.uritemplate="/mnt/overlay/wcm/core/content/sites/createcsvexport.html{+id}"/>
                        </createcsvexport>
                    </items>
                </create>
            </selection>
        </actions>
        <!-- Added for sorting of alphabetical order in sites admin console  -->
        <views jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured">
                <datasource
                        jcr:primaryType="nt:unstructured"
                        sortDir="desc"
                        sortName="name"/>
            </column>
        </views>
    </jcr:content>
</jcr:root>