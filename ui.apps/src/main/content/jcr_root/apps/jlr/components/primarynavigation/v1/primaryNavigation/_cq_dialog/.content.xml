<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Primary Navigation"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[jlr.authoring.dialog]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
        margin="{Boolean}false">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <navPath1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldDescription="Link to a content page, external URL or page anchor."
                                fieldLabel="Link"
                                name="./navPagePathOne"
                                rootPath="/content/landrover"/>
                            <navPath2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldDescription="Link to a content page, external URL or page anchor."
                                fieldLabel="Link"
                                name="./navPagePathTwo"
                                rootPath="/content/landrover"/>
                            <navPath3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldDescription="Link to a content page, external URL or page anchor."
                                fieldLabel="Link"
                                name="./navPagePathThree"
                                rootPath="/content/landrover"/>
                            <primaryNavTitleOne
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="The text to be rendered on the CTA"
                                fieldLabel="Primary Nav Title One"
                                name="./primaryNavTitleOne"/>
                            <primaryNavTitleTwo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="The text to be rendered on the CTA"
                                fieldLabel="Primary Nav Title Two"
                                name="./primaryNavTitleTwo"/>
                            <primaryNavTitleThree
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="The text to be rendered on the CTA"
                                fieldLabel="Primary Nav Title Three"
                                name="./primaryNavTitleThree"/>
                            <primaryNavTitleFour
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="The text to be rendered on the CTA"
                                fieldLabel="Primary Nav Title Four"
                                name="./primaryNavTitleFour"/>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
