<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0" jcr:primaryType="nt:unstructured" jcr:title="Interactive Option Picker" sling:resourceType="cq/gui/components/authoring/dialog">
   <content granite:class="cmp-contentfragmentlist__editor" jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
      <items jcr:primaryType="nt:unstructured">
         <tabs jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/tabs" maximized="{Boolean}true">
            <items jcr:primaryType="nt:unstructured">
               <properties jcr:primaryType="nt:unstructured" jcr:title="Properties" sling:resourceType="granite/ui/components/coral/foundation/container" margin="{Boolean}true">
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <id jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/include" path="/mnt/overlay/jlr/components/globalfields/general/id" />
                           <headerCopy required="{Boolean}true" jcr:primaryType="nt:unstructured" sling:resourceType="cq/gui/components/authoring/dialog/richtext" fieldDescription="The Header copy to be displayed." fieldLabel="Header Copy" name="./globalHeaderCopy" dfs-required="true" useFixedInlineToolbar="{Boolean}true">
                              <rtePlugins jcr:primaryType="nt:unstructured">
                                 <subsuperscript jcr:primaryType="nt:unstructured" features="*" />
                                 <misctools jcr:primaryType="nt:unstructured" features="*">
                                    <specialCharsConfig jcr:primaryType="nt:unstructured">
                                       <chars jcr:primaryType="nt:unstructured">
                                          <non-breaking-space jcr:primaryType="nt:unstructured" entity="&amp;nbsp;" name="Non-breaking space" />
                                          <non-breaking-hyphen jcr:primaryType="nt:unstructured" entity="&amp;#8209" name="Non-breaking hyphen" />
                                       </chars>
                                    </specialCharsConfig>
                                 </misctools>
                              </rtePlugins>
                              <uiSettings jcr:primaryType="nt:unstructured">
                                 <cui jcr:primaryType="nt:unstructured">
                                    <inline jcr:primaryType="nt:unstructured" toolbar="[subsuperscript#subscript,subsuperscript#superscript,misctools#specialchars]" />
                                    <dialogFullScreen jcr:primaryType="nt:unstructured" toolbar="[subsuperscript#subscript,subsuperscript#superscript,misctools#specialchars]" />
                                 </cui>
                              </uiSettings>
                           </headerCopy>
                           <summary jcr:primaryType="nt:unstructured" sling:resourceType="cq/gui/components/authoring/dialog/richtext" fieldDescription="The copy to be displayed." fieldLabel="Summary" name="./globalSummary" dfs-required="true" useFixedInlineToolbar="{Boolean}true">
                              <rtePlugins jcr:primaryType="nt:unstructured">
                                 <format jcr:primaryType="nt:unstructured" features="bold,italic" />
                                 <subsuperscript jcr:primaryType="nt:unstructured" features="*" />
                                 <paraformat jcr:primaryType="nt:unstructured" features="*">
                                    <formats jcr:primaryType="nt:unstructured">
                                       <default_p jcr:primaryType="nt:unstructured" description="Paragraph" tag="p" />
                                       <default_h1 jcr:primaryType="nt:unstructured" description="Heading 1" tag="h1" />
                                       <default_h2 jcr:primaryType="nt:unstructured" description="Heading 2" tag="h2" />
                                       <default_h3 jcr:primaryType="nt:unstructured" description="Heading 3" tag="h3" />
                                       <default_h4 jcr:primaryType="nt:unstructured" description="Heading 4" tag="h4" />
                                       <default_h5 jcr:primaryType="nt:unstructured" description="Heading 5" tag="h5" />
                                       <default_h6 jcr:primaryType="nt:unstructured" description="Heading 6" tag="h6" />
                                       <default_blockquote jcr:primaryType="nt:unstructured" description="Quote" tag="blockquote" />
                                       <default_pre jcr:primaryType="nt:unstructured" description="Preformatted" tag="pre" />
                                    </formats>
                                 </paraformat>
                                 <links jcr:primaryType="nt:unstructured" features="modifylink,unlink" />
                                 <lists jcr:primaryType="nt:unstructured" features="*" />
                                 <misctools jcr:primaryType="nt:unstructured" features="*">
                                    <specialCharsConfig jcr:primaryType="nt:unstructured">
                                       <chars jcr:primaryType="nt:unstructured">
                                          <non-breaking-space jcr:primaryType="nt:unstructured" entity="&amp;nbsp;" name="Non-breaking space" />
                                          <non-breaking-hyphen jcr:primaryType="nt:unstructured" entity="&amp;#8209" name="Non-breaking hyphen" />
                                       </chars>
                                    </specialCharsConfig>
                                 </misctools>
                              </rtePlugins>
                              <uiSettings jcr:primaryType="nt:unstructured">
                                 <cui jcr:primaryType="nt:unstructured">
                                    <inline jcr:primaryType="nt:unstructured" toolbar="[format#bold,format#italic,subsuperscript#superscript,subsuperscript#subscript,#paraformat,links#modifylink,links#unlink,#lists,misctools#specialchars]">
                                       <popovers jcr:primaryType="nt:unstructured">
                                          <lists jcr:primaryType="nt:unstructured" items="[lists#unordered,lists#ordered]" ref="lists" />
                                          <paraformat jcr:primaryType="nt:unstructured" items="paraformat:getFormats:paraformat-pulldown" ref="paraformat" />
                                       </popovers>
                                    </inline>
                                    <dialogFullScreen jcr:primaryType="nt:unstructured" toolbar="[format#bold,format#italic,subsuperscript#superscript,subsuperscript#subscript,#paraformat,links#modifylink,links#unlink,#lists,misctools#specialchars]">
                                       <popovers jcr:primaryType="nt:unstructured">
                                          <lists jcr:primaryType="nt:unstructured" items="[lists#unordered,lists#ordered]" ref="lists" />
                                          <paraformat jcr:primaryType="nt:unstructured" items="paraformat:getFormats:paraformat-pulldown" ref="paraformat" />
                                       </popovers>
                                    </dialogFullScreen>
                                 </cui>
                              </uiSettings>
                           </summary>
                           <text jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldDescription="The text to be rendered on the CTA" fieldLabel="Global CTA Text" name="./globalCtaText" />
                           <link jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser" fieldDescription="Link to a content page, external URL or page anchor." fieldLabel="Global CTA Link" name="./globalCtaLink" rootPath="/content/landrover/global" />
                           <icon jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/select" fieldDescription="Select an icon to be rendered before the text on the CTA" fieldLabel="Global CTA Icon" name="./globalCtaIcon">
                              <datasource jcr:primaryType="nt:unstructured" sling:resourceType="acs-commons/components/utilities/genericlist/datasource" path="/etc/acs-commons/lists/icons" />
                           </icon>
                           <linkType jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" fieldDescription="Link Type for the link authored - to make a link as Primary or Secondary" fieldLabel="Global CTA Link Type" name="./globalCtaLinkType">
                              <items jcr:primaryType="nt:unstructured">
                                 <current jcr:primaryType="nt:unstructured" selected="{Boolean}true" text="Primary" value="primary" />
                                 <new jcr:primaryType="nt:unstructured" text="Secondary" value="secondary" />
                              </items>
                           </linkType>
                           <ariaLabel jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldDescription="Leave blank for automated aria label ({header}:{CTA text})." fieldLabel="Global CTA Aria Label" name="./globalCtaAriaLabel" />
                           <target jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" fieldDescription="Target for the link authored - current tab or same tab" fieldLabel="Target" name="./target">
                              <items jcr:primaryType="nt:unstructured">
                                 <sameWindow jcr:primaryType="nt:unstructured" selected="{Boolean}true" text="Same Window" value="_self" />
                                 <newWindow jcr:primaryType="nt:unstructured" text="New Window" value="_blank" />
                                 <overlay jcr:primaryType="nt:unstructured" text="Overlay" value="overlay" />
                              </items>
                           </target>
                        </items>
                     </column>
                  </items>
                  <granite:data jcr:primaryType="nt:unstructured" maxlimit="1" min-items="{Long}1" interactive-option-picker-component="true" granite:class="blue-border-box" />
               </properties>
               <ioption jcr:primaryType="nt:unstructured" jcr:title="Option Images" sling:resourceType="granite/ui/components/coral/foundation/container" margin="{Boolean}true">
                  <items jcr:primaryType="nt:unstructured">
                     <optionItem jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield" composite="{Boolean}true" fieldLabel="Option Image">
                        <field granite:class="horizontal-line" jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container" name="./optionPickerList">
                           <items jcr:primaryType="nt:unstructured">
                              <well jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/well" granite:class="grey-border">
                                 <items jcr:primaryType="nt:unstructured">
                                    <heading jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/heading" level="{Long}3" text="Main Image" />
                                    <imagePath required="{Boolean}true" jcr:primaryType="nt:unstructured" sling:resourceType="cq/gui/components/authoring/dialog/fileupload" allowUpload="{Boolean}false" fieldDescription="Select an Main Image to be displayed"  granite:class="img-path" fileReferenceParameter="./fileReference" mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]" multiple="{Boolean}false" name="./optionImage" />
                                    <decorative jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/include" path="/mnt/overlay/jlr/components/globalfields/image/decorative" />
                                    <imgAlt jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/include" path="/mnt/overlay/jlr/components/globalfields/image/imageAlt" />
                                 </items>
                              </well>
                              <well-0 granite:class="grey-border" jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/well">
                                 <items jcr:primaryType="nt:unstructured">
                                    <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/container">
                                       <items jcr:primaryType="nt:unstructured">
                                          <heading jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/heading" level="{Long}3" text="Thumbnail Image" />
                                          <imagePath required="{Boolean}true" jcr:primaryType="nt:unstructured" sling:resourceType="cq/gui/components/authoring/dialog/fileupload" allowUpload="{Boolean}false" fieldDescription="Select the thumbnail to be displayed"  granite:class="img-path" fileReferenceParameter="./fileReference" mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]" multiple="{Boolean}false" name="./thumbnailImage" />
                                       </items>
                                    </column>
                                 </items>
                                 <granite:data jcr:primaryType="nt:unstructured" maxlimit="1" />
                              </well-0>
                              <well-1 granite:class="grey-border" jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/well">
                                 <items jcr:primaryType="nt:unstructured">
                                    <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/container">
                                       <items jcr:primaryType="nt:unstructured">
                                          <optionLabel jcr:primaryType="nt:unstructured" sling:resourceType="cq/gui/components/authoring/dialog/richtext" fieldDescription="The option label to be displayed on hover" fieldLabel="Option Label" name="./optionLabel" dfs-required="true" useFixedInlineToolbar="{Boolean}true">
                                             <rtePlugins jcr:primaryType="nt:unstructured">
                                                <format jcr:primaryType="nt:unstructured" features="bold,italic" />
                                                <subsuperscript jcr:primaryType="nt:unstructured" features="*" />
                                                <paraformat jcr:primaryType="nt:unstructured" features="*">
                                                   <formats jcr:primaryType="nt:unstructured">
                                                      <default_p jcr:primaryType="nt:unstructured" description="Paragraph" tag="p" />
                                                      <default_h1 jcr:primaryType="nt:unstructured" description="Heading 1" tag="h1" />
                                                      <default_h2 jcr:primaryType="nt:unstructured" description="Heading 2" tag="h2" />
                                                      <default_h3 jcr:primaryType="nt:unstructured" description="Heading 3" tag="h3" />
                                                      <default_h4 jcr:primaryType="nt:unstructured" description="Heading 4" tag="h4" />
                                                      <default_h5 jcr:primaryType="nt:unstructured" description="Heading 5" tag="h5" />
                                                      <default_h6 jcr:primaryType="nt:unstructured" description="Heading 6" tag="h6" />
                                                      <default_blockquote jcr:primaryType="nt:unstructured" description="Quote" tag="blockquote" />
                                                      <default_pre jcr:primaryType="nt:unstructured" description="Preformatted" tag="pre" />
                                                   </formats>
                                                </paraformat>
                                                <links jcr:primaryType="nt:unstructured" features="modifylink,unlink" />
                                                <lists jcr:primaryType="nt:unstructured" features="*" />
                                                <misctools jcr:primaryType="nt:unstructured" features="*">
                                                   <specialCharsConfig jcr:primaryType="nt:unstructured">
                                                      <chars jcr:primaryType="nt:unstructured">
                                                         <non-breaking-space jcr:primaryType="nt:unstructured" entity="&amp;nbsp;" name="Non-breaking space" />
                                                         <non-breaking-hyphen jcr:primaryType="nt:unstructured" entity="&amp;#8209" name="Non-breaking hyphen" />
                                                      </chars>
                                                   </specialCharsConfig>
                                                </misctools>
                                             </rtePlugins>
                                             <uiSettings jcr:primaryType="nt:unstructured">
                                                <cui jcr:primaryType="nt:unstructured">
                                                   <inline jcr:primaryType="nt:unstructured" toolbar="[format#bold,format#italic,subsuperscript#superscript,subsuperscript#subscript,#paraformat,links#modifylink,links#unlink,#lists,misctools#specialchars]">
                                                      <popovers jcr:primaryType="nt:unstructured">
                                                         <lists jcr:primaryType="nt:unstructured" items="[lists#unordered,lists#ordered]" ref="lists" />
                                                         <paraformat jcr:primaryType="nt:unstructured" items="paraformat:getFormats:paraformat-pulldown" ref="paraformat" />
                                                      </popovers>
                                                   </inline>
                                                   <dialogFullScreen jcr:primaryType="nt:unstructured" toolbar="[format#bold,format#italic,subsuperscript#superscript,subsuperscript#subscript,#paraformat,links#modifylink,links#unlink,#lists,misctools#specialchars]">
                                                      <popovers jcr:primaryType="nt:unstructured">
                                                         <lists jcr:primaryType="nt:unstructured" items="[lists#unordered,lists#ordered]" ref="lists" />
                                                         <paraformat jcr:primaryType="nt:unstructured" items="paraformat:getFormats:paraformat-pulldown" ref="paraformat" />
                                                      </popovers>
                                                   </dialogFullScreen>
                                                </cui>
                                             </uiSettings>
                                          </optionLabel>
                                          <text jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/include" path="/mnt/overlay/jlr/components/globalfields/cta/text" />
                                          <link jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/include" path="/mnt/overlay/jlr/components/globalfields/cta/link" />
                                          <icon jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/include" path="/mnt/overlay/jlr/components/globalfields/cta/icon" />
                                          <linkType jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/include" path="/mnt/overlay/jlr/components/globalfields/cta/linkType" />
                                          <ariaLabel jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/include" path="/mnt/overlay/jlr/components/globalfields/cta/ariaLabel" />
                                          <target jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/include" path="/mnt/overlay/jlr/components/globalfields/cta/targetTab" />
                                       </items>
                                    </column>
                                 </items>
                              </well-1>
                           </items>
                        </field>
                        <granite:data jcr:primaryType="nt:unstructured" maxlimit="13" min-items="{Long}0" vertical-slider-component="true" />
                     </optionItem>
                  </items>
               </ioption>
            </items>
         </tabs>
      </items>
   </content>
</jcr:root>