<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Vehicle Summary(v1)"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <styletab
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/mnt/overlay/cq/gui/components/authoring/dialog/style/tab_design/styletab"/>
                    <others
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Others"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                maximized="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <edit
                                                granite:class="js-cq-IPEPlugin-group"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <paste-plaintext
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Show paste plain text option"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                        name="paste-plaintext"/>
                                                    <paste-wordhtml
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Show paste from word option"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                        name="paste-wordhtml"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    name="edit"
                                                    plugin-root-name="rtePlugins"/>
                                            </edit>
                                            <find-replace
                                                granite:class="js-cq-IPEPlugin-group"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <find-replace
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Show find &amp; replace options"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                        features="*"
                                                        name="findreplace"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    plugin-root-name="rtePlugins"/>
                                            </find-replace>
                                            <undo-redo
                                                granite:class="js-cq-IPEPlugin-group"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <undo
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Show undo &amp; redo options"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                        features="*"
                                                        name="undo"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    plugin-root-name="rtePlugins"/>
                                            </undo-redo>
                                            <spellcheck
                                                granite:class="js-cq-IPEPlugin-group"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <spellcheck
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Show spell checker options"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                        features="*"
                                                        name="spellcheck"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    plugin-root-name="rtePlugins"/>
                                            </spellcheck>
                                            <editHTML
                                                granite:class="js-cq-IPEPlugin-group"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <editHTML
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Show HTML source editing option"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                        name="sourceedit"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    name="misctools"
                                                    plugin-root-name="rtePlugins"/>
                                            </editHTML>
                                            <text
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                                path="/mnt/overlay/jlr/components/globalfields/cta/text"/>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </others>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
