<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Price Config"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
        margin="{Boolean}false">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <nameplatelist
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldDescription="List of the Nameplates to be fetched"
                                fieldLabel="Nameplate List">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                    name="./nameplateList">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <nameplateid
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Nameplate ID"
                                                    name="./nameplateId"
                                                    required="{Boolean}true"/>
                                                <modelyear
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    fieldDescription="Author the multiple model year for the nameplate ID"
                                                    fieldLabel="Model year"
                                                    multiple="{Boolean}true"
                                                    name="./modelYear"
                                                    required="{Boolean}true">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <_x0032_018
                                                            jcr:primaryType="nt:unstructured"
                                                            text="2018"
                                                            value="k18"/>
                                                        <_x0032_019
                                                            jcr:primaryType="nt:unstructured"
                                                            text="2019"
                                                            value="k19"/>
                                                        <_x0032_020
                                                            jcr:primaryType="nt:unstructured"
                                                            text="2020"
                                                            value="k20"/>
                                                    </items>
                                                </modelyear>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </nameplatelist>
                        </items>
                    </general>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
