<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Ready to go bar"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="jlr.component.rtgb.minmax">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <id
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="/mnt/overlay/jlr/components/globalfields/general/id"/>
                            <enableFAB
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                defaultChecked="{Boolean}true"
                                fieldDescription="If checked 'Floating Action Bar' will be visible"
                                name="./enableFAB"
                                text="Enable Floating Action Bar"
                                value="{Boolean}true"/>
                        </items>
                    </properties>
                    <rtgbList
                        jcr:primaryType="nt:unstructured"
                        jcr:title="RTGB List"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <rtgb
                                granite:class="minmax"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldDescription="Click 'add' to add a new accessibility Label and Link"
                                fieldLabel="Ready to go bar List"
                                validation="minmax">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./rtgb">
                                    <items jcr:primaryType="nt:unstructured">
                                        <icon
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                            path="/mnt/overlay/jlr/components/globalfields/image/imagePath"/>
                                        <imageAlt
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                            path="/mnt/overlay/jlr/components/globalfields/image/imageAlt"/>
                                        <imageLink
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                            path="/mnt/overlay/jlr/components/globalfields/image/imageLink"/>
                                        <text
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                            path="/mnt/overlay/jlr/components/globalfields/cta/text"/>
                                        <link
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                            path="/mnt/overlay/jlr/components/globalfields/cta/link"/>
                                        <linkType
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                            path="/mnt/overlay/jlr/components/globalfields/cta/linkType"/>
                                        <target
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                            path="/mnt/overlay/jlr/components/globalfields/cta/targetTab"/>
                                        <description
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                            path="/mnt/overlay/jlr/components/globalfields/general/copy"/>
                                    </items>
                                </field>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    max-items="{Long}4"
                                    min-items="{Long}4"/>
                            </rtgb>
                        </items>
                    </rtgbList>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
